<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.StackPane?>


<StackPane fx:id="ratStackPane" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pfariasmunoz.ratmanager.ui.RatViewController">
   <children>
      <Label fx:id="ratTitle" alignment="TOP_LEFT" text="Manage the data from the experiment" StackPane.alignment="TOP_LEFT">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <StackPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </StackPane.margin>
      </Label>
      <TableView fx:id="ratTable" prefHeight="382.0" prefWidth="624.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
         <StackPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="50.0" />
         </StackPane.margin>
      </TableView>
      <Button mnemonicParsing="false" text="Add Rat" StackPane.alignment="TOP_RIGHT">
         <StackPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </StackPane.margin>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </Button>
   </children>
</StackPane>
